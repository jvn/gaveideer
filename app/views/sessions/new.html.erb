<h1>LogiNz</h1>

<%= form_tag brugers_path do %>

    <p>
      <%= label_tag :email %><br />
      <%= text_field_tag :email, params[:email] %>
    </p>

    <p>
      <%= label_tag :password %><br />
      <%= text_field_tag :password %>
    </p>

    <p class = "button"><%= submit_tag "login" %> </p>

    <br />
    <%= link_to 'Ny Bruger', new_bruger_path %>
<% end %>
